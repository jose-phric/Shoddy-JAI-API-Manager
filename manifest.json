{
  "manifest_version": 3,
  "name": "Button Injector",
  "version": "1.0",
  "description": "Injects custom buttons into a specific element.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Custom Setups"
  },
  "content_scripts": [
    {
      "matches": ["https://janitorai.com/chats/*"],
      "js": ["content.js"]
    }
  ]
}
